<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd"
                   logicalFilePath="migration/node-services.changelog-init.xml">

    <changeSet author="R3.Corda" id="lower-case-column-names-for-node_mutual_exclusion">
        <preConditions onFail="MARK_RAN" onSqlOutput="TEST">
            <dbms type="mssql" />
            <dbms type="azure" />
        </preConditions>
        <renameColumn tableName="node_mutual_exclusion" oldColumnName="MACHINE_NAME" newColumnName="machine_name"/>
        <renameColumn tableName="node_mutual_exclusion" oldColumnName="PID" newColumnName="pid"/>
        <renameColumn tableName="node_mutual_exclusion" oldColumnName="MUTUAL_EXCLUSION_TIMESTAMP" newColumnName="mutual_exclusion_timestamp"/>
    </changeSet>
</databaseChangeLog>
