<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <changeSet author="R3.Corda" id="persist_map_test">
        <createTable tableName="persist_map_test">
            <column name="key" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="value" type="NVARCHAR(16)">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addPrimaryKey columnNames="key" constraintName="key_pkey" tableName="persist_map_test"/>
    </changeSet>
</databaseChangeLog>